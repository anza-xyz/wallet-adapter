{
    "name": "@solana/wallet-adapter-example",
    "version": "0.3.0",
    "author": "Solana Maintainers <maintainers@solana.foundation>",
    "repository": "https://github.com/solana-labs/wallet-adapter",
    "license": "Apache-2.0",
    "files": [
        "build",
        "src"
    ],
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
