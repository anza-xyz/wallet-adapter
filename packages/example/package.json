        "start": "yarn clean && parcel serve --dist-dir build src/index.html"
    },
    "dependencies": {
        "@material-ui/core": "^4.12.1",
        "@solana/wallet-adapter-base": "^0.3.0",
        "@solana/wallet-adapter-material-ui": "^0.3.0",
        "@solana/wallet-adapter-react": "^0.3.0",
        "@solana/wallet-adapter-wallets": "^0.3.0",
        "@solana/web3.js": "^1.20.0",
        "notistack": "^1.0.9",
        "react": "^17.0.2",
        "react-dom": "^17.0.2"
    },
    "devDependencies": {
        "@parcel/transformer-typescript-tsc": "^2.0.0-beta.3.1",
        "@types/react": "^17.0.0",
        "@types/react-dom": "^17.0.0",
        "parcel": "^2.0.0-beta.3.1"
    }
}
